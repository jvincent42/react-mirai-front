{
  "name": "node-mirai",
  "version": "0.1.0",
  "description": "futuristic babel configuration",
  "repository": "https://ato42@bitbucket.org/ato42/node-mirai.git",
  "main": "lib/index.js",
  "scripts": {
    "start": "node lib/index.js",
    "start:dev": "NODE_ENV=development webpack-dev-server --hot",
    "dev": "NODE_ENV=development nodemon -r @babel/register ./src/index.js",
    "clean": "rimraf lib/",
    "build": "NODE_ENV=production babel src/ --out-dir lib/",
    "lint": "eslint src",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "jvincent",
  "license": "MIT",
  "dependencies": {
    "ramda": "^0.25.0",
    "react": "^16.3.1",
    "react-dom": "^16.3.1",
    "react-hot-loader": "^4.0.1",
    "webpack": "^4.5.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.44",
    "@babel/core": "^7.0.0-beta.44",
    "@babel/node": "^7.0.0-beta.44",
    "@babel/plugin-proposal-function-bind": "^7.0.0-beta.44",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0-beta.44",
    "@babel/plugin-proposal-pipeline-operator": "^7.0.0-beta.44",
    "@babel/preset-env": "^7.0.0-beta.44",
    "@babel/preset-react": "^7.0.0-beta.44",
    "@babel/preset-stage-0": "^7.0.0-beta.44",
    "@babel/register": "^7.0.0-beta.44",
    "babel-eslint": "^8.2.2",
    "babel-loader": "^8.0.0-beta.2",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.10.0",
    "eslint-plugin-react": "^7.7.0",
    "html-webpack-plugin": "^3.2.0",
    "nodemon": "^1.17.3",
    "rimraf": "^2.6.2",
    "webpack-cli": "^2.0.14",
    "webpack-dev-server": "^3.1.1"
  }
}
