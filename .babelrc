{
  "presets": [
    ["@babel/preset-env", {
      "exclude": ["transform-regenerator"]
    }],
    "@babel/preset-stage-0",
    "@babel/preset-react"
  ],
  "plugins": [
    "@babel/plugin-proposal-function-bind",
    "@babel/plugin-proposal-pipeline-operator",
    "@babel/plugin-proposal-optional-chaining",
    "react-hot-loader/babel"
  ]
}
